{
  "name": "@shadowcms/client",
  "version": "6.0.12",
  "description": "Main Frontend Client for ShadowCMS",
  "main": "index.js",
  "scripts": {
    "dev": "set PORT=4738 && react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "lint": "eslint --fix --ext .jsx,.js,.tsx,.ts ./src/",
    "test": "jest",
    "clean": "rm -rf build/",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadowcms/shadow.git"
  },
  "author": "@shadowcms",
  "license": "MIT",
  "homepage": "https://shadowcms.com",
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": "eslint --cache --fix "
  },
  "devDependencies": {
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  }
}
